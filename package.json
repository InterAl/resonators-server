{
  "name": "resonators-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": ". ~/.nvm/nvm.sh && nvm use && ENV=dev node index.js",
    "win-start": "nvm use && SET ENV=dev && node index.js",
    "test": ". ~/.nvm/nvm.sh && nvm use && ./test/run -- --watch test/**/*.test.js",
    "win-test": "nvm use && ./test/run -- --watch test/**/*.test.js",
    "test:noinit": ". ~/.nvm/nvm.sh && nvm use && ./test/run noinit -- --watch test/**/*.test.js",
    "test:watchSingle": ". ~/.nvm/nvm.sh && nvm use && ./test/run noinit -- --watch",
    "lint": "eslint ./src",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "model:create": "sequelize model:create --underscored --name table_name --attributes 'field_1:string, field_2:string'",
    "initdb": "./scripts/initdb",
    "win-initdb": "node scripts\\initdb",
    "storybook": "start-storybook -p 9007 -c .storybook",
    "start:prod": "sudo NODE_ENV=production ENV=production ./node_modules/.bin/pm2 -f start index.js --name resonators",
    "restart:prod": "sudo pm2 restart resonators"
  },
  "author": "InterAl",
  "license": "ISC",
